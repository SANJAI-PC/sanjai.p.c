import java.util.Scanner;
public class Main
{
	public static void main(String[] args) 
	{
	    int a;
	    Scanner s=new Scanner(System.in);
		System.out.println("enter minutes: ");
		a=s.nextInt();
		yearsanddays(a);
	}
	
	public static void yearsanddays(int a)
	{
	    int b,c,d;
	    b=a/525600;
        c=(a-(b*525600));
        d=c/1440;
	    System.out.println(a+" minutes is approximately "+b+" years and "+d+" days");
	}
}
